{
    "description": "Demonstrating webRequests",
    "manifest_version": 2,
    "name": "webRequest-demo",
    "version": "1.0",
  
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "storage",
      "tabs",
      "notifications",
      "activeTab",
      "contextMenus",
      "unlimitedStorage",
      "clipboardRead",
      "downloads",
      "clipboardWrite",
      "<all_urls>"
    ],
  
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }
  